type Post {
  id: ID! @id
  price: Int!
  position: String!
  industry: Industry!
  description: String!
  tags: [Tag]!
  # coachId: ID!
  # ratingsId: ID!
}

type Industry {
	id: ID! @id
	name: String! @unique
  posts: [Post]!
}

type Tag {
  id: ID! @id
  name: String! @unique
  posts: [Post]!
}
