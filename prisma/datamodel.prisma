type Post {
  id: ID! @id
  price: Int!
  position: String!
  industry: Industry!
  description: String!
  coachID: String! @unique
  # coach: User @provides(fields: "email") @relation(link: TABLE name: "UserOnPost", onDelete: SET_NULL)
  tags: [Tag]!
  # ratingsId: ID!
}

type Industry {
	id: ID! @id
	name: String! @unique
  posts: [Post]!
}

type Tag {
  id: ID! @id
  name: String! @unique
  posts: [Post]!
}

