type Post {
  id: ID! @id
  price: Int!
  position: String!
  industry: Industry!
  description: String!
  coachID: String! @unique
  # coach: User @provides(fields: "email") @relation(link: TABLE name: "UserOnPost", onDelete: SET_NULL)
  tags: [Tag]!
  # ratingsId: ID!
}
type Industry {
	id: ID! @id
	name: String! @unique
  posts: [Post]!
}
type Tag {
  id: ID! @id
  name: String! @unique
  posts: [Post]!
}

type Availability {
  id: ID! @id
  dayOfWeek: String!
  start_hour: Int!
  start_minute: Int!
  end_hour: Int!
  end_minute: Int!
  coach: String!
}

type Booking {
  id: ID! @id
  year: Int!
  month: Int!
  day: Int!
  hour: Int!
  minute: Int!
  coach: String!
  seeker: String!
}

